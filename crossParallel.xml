  </test> <!-- Test -->
  
  <test  name="edgeTest">
  <parameter name="Browser" value="edge"></parameter>
    <classes>
      <class name="com.HybridFramework.TestCases.RegistrationTest"/>
    </classes>
  </test> <!-- Test -->

	<test name="firefoxTest">
	<parameter name="Browser" value="firefox"></parameter>
    <classes>
      <class name="com.HybridFramework.TestCases.RegistrationTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
